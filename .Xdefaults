URxvt*.scrollColor:				grey
URxvt*.troughColor:				black

URxvt*.font:					xft:Inconsolata for Powerline:antialias=true:pixelsize=16, xft:Unifont:antialias=true:pixelsize=16, xft:Ubuntu Mono derivative Powerline:style=Regular:antialias=true:pixelsize=16
URxvt*.boldFont:				xft:Inconsolata for Powerline:bold:antialias=true:pixelsize=16, xft:Unifont:antialias=true:pixelsize=16, xft:Ubuntu Mono derivative Powerline:style=Bold:antialias=true:pixelsize=16
URxvt*.italicFont:				xft:Inconsolata for Powerline:italic:antialias=true:autohint=true:pixelsize=16, xft:Unifont:antialias=true:pixelsize=16, xft:Ubuntu Mono derivative Powerline:style=Italic:antialias=true:pixelsize=16
URxvt*.boldItalicFont:			xft:Inconsolata for Powerline:italic:antialias=true:autohint=true:pixelsize=16, xft:Unifont:antialias=true:pixelsize=16, xft:Ubuntu Mono derivative Powerline:style=Bold Italic:antialias=true:pixelsize=16

URxvt*.foreground:				white
URxvt*.background:				black
URxvt*.geometry:				150x30
URxvt*.jumpScroll:				true
URxvt*.skipScroll:				true

! NATIVE TRANSPARENCY
! shading values are 0-200, > 100 lighter, < 100 darker
URxvt*.transparent:				true
URxvt*.shading:					90

URxvt*.fading:					20
URxvt*.tintColor:				grey30
URxvt*.mapAlert:				true
URxvt*.loginShell:				true
URxvt*.scrollstyle:				rxvt
URxvt*.scrollBar:				true
URxvt*.scrollTtyOutput:			true
URxvt*.scrollWithBuffer:		true
URxvt*.scrollTtyKeypress:		true
URxvt*.saveLines:				1048576
URxvt*.internalBorder:			5
URxvt*.externalBorder:			0
URxvt*.pastableTabs:			true
URxvt*.pointerBlank:			true
URxvt*.pointerBlankDelay:		5
URxvt*.tripleclickwords:		true

URxvt*.multiScroll:				true
URxvt*.scrollKey:				true

URxvt*.perl-ext-common:			default,matcher
URxvt*.urlLauncher:				/usr/bin/firefox-nightly
URxvt*.matcher.button:			1
URxvt*.scrollBar_floating:		true
URxvt*.scrollBar_right:			true

URxvt*iso14755:				false

! TRUE TRANSPARENCY	... requires (if not using a wm w/ compositing mgr) something like compton
!URxvt*.depth: 			32
!URxvt*.background: 		[84]#000000

Xcursor.theme:          	gentoo-silver

!Xft.dpi:               	75

!xterm*termName: 		xterm-256color

